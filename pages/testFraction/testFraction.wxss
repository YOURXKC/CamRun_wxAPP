/* pages/testFraction/testFraction.wxss */
.views{
  width: 100%;
  min-height: 100vh;
  background-color: #6A5ACD;
  overflow-x: hidden;
}
.single{
  width: 95vw;
  height: auto;
  background-color: white;
  margin: 50rpx auto;
  border-radius: 25rpx;
  overflow: hidden;
}
.year-semester{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 42rpx;
  background-color: rgb(37, 186, 245);
  border: 2rpx solid rgb(37, 186, 245);
  color: white;
}
.year-semester text{
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 20rpx;
}
.button-img{
  display: block;
  float: right;
  margin: 20rpx 20rpx 0 0;
  width: 60rpx;
  height: 60rpx;
}
.details{
  height: 90rpx;
  width: 95%;
  margin: 0 auto;
  font-size: 35rpx;
  border-bottom: 2rpx solid rgb(200,200,200);
}
.name{
  float: left;
  width: 80%;
  height: 90rpx;
  line-height: 90rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.fraction{
  float: right;
  width: 20%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
}
.fraction_details{
  overflow-x: hidden;
  width: 100%;
  height: auto;
  padding-top: 10rpx;
  padding-bottom: 20rpx;
}
.tishi{
  width: 100%;
  height: 100vh;
  color: white;
  line-height: 100vh;
  text-align: center;
  font-size: 35rpx;
}